{
    "name": "antfroger/progressive-bundle",
    "description": "The bundle to load Progressive, the library to progressively, quickly and simply enable new features",
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/antfroger/progressive-bundle",
    "authors": [
        {
            "name": "Antoine Froger",
            "email": "antfroger@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "antfroger/progressive": "^1.2",
        "symfony/config": "^5.4||^6.4||^7.0",
        "symfony/console": "^5.4||^6.4||^7.0",
        "symfony/dependency-injection": "^5.4||^6.4||^7.0",
        "symfony/http-kernel": "^5.4||^6.4||^7.0",
        "symfony/security-core": "^5.4||^6.4||^7.0",
        "symfony/var-dumper": "^5.4||^6.4||^7.0",
        "symfony/yaml": "^5.4||^6.4||^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": { "Af\\ProgressiveBundle\\": "" }
    },
    "minimum-stability": "stable"
}
